<Window x:Class="Face.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Face"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="70"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Button x:Name="BntBack" HorizontalAlignment="Right" Background="Red" Foreground="White">Назад</Button>
        <StackPanel Orientation="Horizontal">
            <Image Source="Resources/beauty_logo.png" Margin="10" VerticalAlignment="Center" HorizontalAlignment="Left" Height="70"/>
            <TextBlock VerticalAlignment="Center" Foreground="Red">Мордочка</TextBlock>
        </StackPanel>
    </Grid>
</Window>
